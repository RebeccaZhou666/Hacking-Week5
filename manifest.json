{
    "name": "Love Calculator Tab",
    "short_name": "Cus. Tabs",
    "description": "Cute default tabs.",
    "version": "1.0",
    "icons": {
        "16": "dist/images/icons/icon16.png",
        "48": "dist/images/icons/icon48.png",
        "128": "dist/images/icons/icon128.png"
    },
    "background": {
        "scripts": [
            "dist/js/jquery.min.js",
            "dist/js/background.js"
        ],
        "persistent": false
    },
    "chrome_url_overrides": {
        "newtab": "dist/html/override.html"
    },
    "permissions": [
        "storage",
        "identity",
        "identity.email"
    ],
    "web_accessible_resources": [
        "dist/js/*",
        "dist/html/override.html",
        "dist/pens/*"
    ],
    "browser_action": {
        "default_popup": "dist/html/popup.html"
    },
    "offline_enabled": true,
    "manifest_version": 2
}
